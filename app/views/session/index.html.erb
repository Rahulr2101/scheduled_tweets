<h1>login</h1>

<%= form_with url: sign_in_path do |form| %>
<div class="mt-3"><%= form.text_field :email, class:"form-control",placeholder: "Email" %></div>
<div class="mt-3"><%= form.password_field :password, class:"form-control",placeholder:"Password" %></div>
<div class="mt-3"><%= form.button "Login",class:"btn btn-primary"%></div>
<% end%>
